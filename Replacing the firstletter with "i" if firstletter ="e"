import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    System.out.println("Replacing the first letter='e' with 'i");
	    Scanner sc= new Scanner(System.in);
	    String username = sc.next();
	    String oen="";
	    for (int i=0;i<username.length();i++ ){
	        if(username.charAt(i)=='e'){
	            oen=oen+'i';
	            
	        } else{
	            oen+=username.charAt(i);
	        }
	        
	        
	    }
	    System.out.println(oen); 
	    
		
	}
}


